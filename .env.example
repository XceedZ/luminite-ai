# API Configuration
# Backend API URL (untuk proxy di next.config.ts)
NEXT_PUBLIC_API_URL=http://localhost:5005

# App URL (untuk production)
NEXT_PUBLIC_APP_URL=http://localhost:8080

# Supabase Configuration (optional)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google AI API
GOOGLE_API_KEY=your_google_ai_api_key

# Gemini API (alternatif untuk Google AI)
GEMINI_API_KEY=your_gemini_api_key

# Upstash Redis
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token

# Pexels API (untuk App Builder image features)
PEXELS_API_KEY=your_pexels_api_key

# Vercel (auto-set oleh Vercel, tidak perlu di-set manual)
# VERCEL_PROJECT_PRODUCTION_URL=your_vercel_production_url
